/* 改变主题色变量 */
$--color-primary: #fc2b47;

/* 改变 icon 字体路径变量，必需 */
$--font-path: '~element-ui/lib/theme-chalk/fonts';

@import "~element-ui/packages/theme-chalk/src/index";

// Pagination-分页器
.el-pagination {
  display: flex;
  align-items: center;
  margin-bottom: 40px;
  .btn-prev {
    height: 34px;
    width: 40px;
  }
  .el-pager {
    li {
      height: 34px;
      width: 34px;
      line-height: 34px;
    }
  }
  .btn-next {
    height: 34px;
    width: 40px;
  }
  .el-pagination__jump {
    height: 38px;
    line-height: 38px;
  }
}
// Select选择器-地址选择
.el-input__inner {
  height: 30px;
  line-height: 30px;
  padding: 0 12px;
}
.el-input__suffix {
  display: flex;
  align-items: center;
}


